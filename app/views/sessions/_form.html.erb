<%= form_for :session, class: "user", url: session_path do |form| %>
  <div class="form-group">
    <%= form.email_field :email, class: "form-control form-control-user", id: "exampleInputEmail", placeholder: "Enter Email Address" %>
  </div>

  <div class="form-group">
    <%= form.password_field :password, class: "form-control form-control-user", id: "exampleInputPassword", placeholder: "Password" %>
  </div>

  <%# <div class="submit-field"> %>
    <%= form.submit "Login", class: "btn btn-primary btn-user btn-block" %>
  <%# </div> %>
<% end %>
